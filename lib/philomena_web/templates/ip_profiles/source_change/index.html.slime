h2 Listing Source changes

table.table
  thead
    tr
      th Order
      th
  tbody
    = for source_change <- @source_changes do
      tr
        td= source_change.order
        td class="text-right"
          = link "Show", to: Routes.ip_profiles_source_change_path(@conn, :show, source_change), class: "btn btn-default btn-xs"
          | &nbsp;
          = link "Edit", to: Routes.ip_profiles_source_change_path(@conn, :edit, source_change), class: "btn btn-default btn-xs"
          | &nbsp;
          = link "Delete", to: Routes.ip_profiles_source_change_path(@conn, :delete, source_change), method: :delete, data: [confirm: "Are you sure?"], class: "btn btn-danger btn-xs"

= link "New Source change", to: Routes.ip_profiles_source_change_path(@conn, :new)
